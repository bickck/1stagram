<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- FileMapper.xml -->

<mapper namespace="com.team.Imitation.repository.MemberMapper">

	<resultMap type="Member" id="memberMap">
		<result column="username" property="username" jdbcType="VARCHAR2" javaType="String"/>
		<result column="email" property="email" jdbcType="VARCHAR2" javaType="String"/>
		<result column="password" property="password" jdbcType="VARCHAR2" javaType="String"/>
		<result column="birth" property="birth" jdbcType="VARCHAR2" javaType="String"/>
		<result column="address" property="address" jdbcType="VARCHAR2" javaType="String"/>
	</resultMap>

	<insert id="register">
		insert into users values(#{username}, #{email}, #{password},#{birth},#{address}, sysdate)
	</insert>

</mapper>


