<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- FileMapper.xml -->

<mapper namespace="com.team.Imitation.repository.MemberMapper">

	<resultMap type="com.team.Imitation.repository.dto.MemberDTO" id="member">
		<result property="username" column="username"/>
		<result property="email" column="email"/>
		<result property="password" column="password"/>
		<result property="birth" column="birth"/>
		<result property="address" column="address"/>
	</resultMap>
	
	<insert id="register">
		insert into membership values(#{username}, #{email}, #{password}, #{birth}, #{address}, sysdate)
	</insert>
	
</mapper>


